<AnchorPane prefHeight="700.0" prefWidth="1000.0" stylesheets="@login_Stylesheet.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="canopy.app.view.noteDialogController">
   <children>
      <BorderPane layoutX="238.0" layoutY="107.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <AnchorPane styleClass="card" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Label fx:id="date" alignment="CENTER" contentDisplay="CENTER" styleClass="label-header" text="N E W   N O T E" textAlignment="CENTER" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </top>
         <center>
            <GridPane hgap="10.0" styleClass="background-white" vgap="5.0" BorderPane.alignment="CENTER_RIGHT">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="173.0" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="800.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="5.0" prefWidth="5.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" percentHeight="0.0" prefHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" percentHeight="0.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" percentHeight="0.0" vgrow="SOMETIMES" />
                  <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
                  <RowConstraints percentHeight="0.0" prefHeight="400.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="-Infinity" percentHeight="0.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label alignment="CENTER_RIGHT" contentDisplay="CENTER" prefHeight="31.0" prefWidth="192.0" styleClass="label-grey" text="N O T E   T Y P E" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
                  <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
                     <children>
                        <ComboBox fx:id="typepicker" prefWidth="350.0" styleClass="table-view" />
                     </children>
                  </HBox>
                  <ButtonBar buttonMinWidth="0.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="6">
                     <buttons>
                        <Button mnemonicParsing="false" onAction="#handleOk" onMouseClicked="#handleOk" text="Add Note" />
                        <Button mnemonicParsing="false" onAction="#handleCancel" onMouseClicked="#handleCancel" text="Cancel" />
                     </buttons>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="15.0" top="10.0" />
                     </padding>
                  </ButtonBar>
                  <Label alignment="TOP_CENTER" contentDisplay="CENTER" styleClass="label-grey" text="N O T E   T E X T" GridPane.halignment="RIGHT" GridPane.rowIndex="5" GridPane.valignment="CENTER" />
                  <TextArea fx:id="textfield" prefHeight="200.0" prefWidth="200.0" styleClass="table-view" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="3">
                     <children>
                        <HBox alignment="CENTER_LEFT" minWidth="0.0">
                           <children>
                              <Button fx:id="filebutton" minWidth="0.0" mnemonicParsing="false" onAction="#addfile" text="ADD FILE">
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Label fx:id="filename" minWidth="0.0" styleClass="label-grey">
                                 <HBox.margin>
                                    <Insets left="15.0" />
                                 </HBox.margin>
                              </Label>
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <Label alignment="TOP_CENTER" contentDisplay="CENTER" styleClass="label-grey" text="N O T E   F I L E" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
                  <Label alignment="CENTER_RIGHT" contentDisplay="CENTER" prefHeight="31.0" prefWidth="192.0" styleClass="label-grey" text="P A T I E N T" GridPane.rowIndex="1" />
                  <Label fx:id="patientname" minWidth="0.0" styleClass="label-dark" text="Patient Name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </center>
      </BorderPane>
      <AnchorPane fx:id="coverpane" styleClass="card">
         <children>
            <VBox alignment="CENTER" layoutX="151.0" layoutY="165.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" styleClass="label-header" text="S A V I N G" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
